class C
{
	construct()
	{
		printf("C.construct (%p)\n", this);
	}

	destruct()
	{
		printf("C.destruct (%p)\n", this);
	}

	void dispose()
	{
		printf("C.dispose (%p)\n", this);
	}
}

void foo()
{
	printf("foo () is about to throw...\n");
	std.setError("foo error");
	throw;
}

int main()
{
	printf("main\n");

	disposable C c1;

	foo();

	disposable C c2;

	printf("*** shouldn't get here\n");
	return -1;

catch:
	printf("main catch\n");
	return 0;

finally:
	printf("main finally\n");
}

