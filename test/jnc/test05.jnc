// test for logical '&&' and '||'

//..............................................................................

int main() {
	printf("main ()\n");

	int a = 5, b = 2, c = 3;

	if (a < c || b < c)
		printf("yuppie!\n");
	else
		printf("oh-oh!\n");

	return 0;
}

