import "std_Array.jnc"

struct S // not going to fit in variant in-place
{
	int64_t m_a;
	int64_t m_b;
	int64_t m_c;
};

int main()
{
	std.Array a;

	for (size_t i = 0; i < 10; i++)
	{
		S s = { i, i * 10, i * 100 };
		a.append(s);
	}

	for (size_t i = 0; i < a.m_count; i++)
	{
		S s = a[i];
		printf("s = { %lld, %lld, %lld }\n", s.m_a, s.m_b, s.m_c);
	}

	return 0;
}
