enum XxxEventCode
{
	ConnectCompleted,
	ConnectCancelled,
	ConnectError,
	Disconnected,
	IncomingData,
	IncomingConnection,
}

// . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .

struct XxxEventParams
{
	XxxEventCode m_eventCode;
	int m_syncId;
}

// . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .

struct XxxErrorEventParams: XxxEventParams
{
	std.Error const* m_error;
}

int main()
{
	XxxEventParams const* params;
	dynamic(XxxErrorEventParams const*) params;

	return 0;
}

