struct Point
{
	int m_x;
	int m_y;

	construct()
	{
		m_x = 10;
		m_y = 20;
	}
}

int main()
{
	printf("main\n");

	Point* point = new Point { m_y = 300 };

	printf($"point = { %1, %2 }\n" (point.m_x, point.m_y));

	return 2;
}

