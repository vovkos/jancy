void foo(char const* const* table) {
	printf("dynamic countof = %d\n", dynamic countof (table));
}

int main() {
	printf("main ()\n");

	char const* a[] = {
		"a",
		"b",
		"c",
	}

	foo(a);

	return 0;
}

