class C2
{
	int get (int i)
	{
		printf ("C2.get (%d)\n", i);
		return i * 10;
	}

	set (
		int i, 
		int value
		)
	{
		printf ("C2.get (%d, %d)\n", i, value);
	}
}

int main ()
{
	printf ("main ()\n");

	C2 c;
	c [10] = 100;
	int x = c [20];
	printf ("x = %d\n", x);

	return 0;
}

