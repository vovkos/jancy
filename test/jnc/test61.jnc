class Resource {
	construct() {
		printf("Resource.construct (%p)\n", this);
	}

	destruct() {
		printf("Resource.destruct (%p)\n", this);
	}

	void close() {
		printf("Resource.close (%p)\n", this);
	}

	alias dispose = close;
}

int main() {
	printf("main\n");

	try {
		disposable Resource resource1;
		disposable Resource resource2;

		int i = 0;
		if (i)
			return -1;

	catch:
		printf("catch 1\n");

	finally:
		printf("finally 1\n");
	}

	printf("hmmm\n");
	return 0;
}

