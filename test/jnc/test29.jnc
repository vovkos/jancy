class C1
{
	static int m_table [] =
	{
		10,
		20,
		30,
	}

	static construct ()
	{
		printf ("C1.static construct ()\n");
	}

	preconstruct ()
	{
		printf ("C1.preconstruct ()\n");
	}

	construct ()
	{
		printf ("C1.construct ()\n");
	}

	foo ()
	{
		printf ("m_table [0] = %d\n", m_table [0]);
	}
}

int main ()
{
	printf ("main ()\n");

	C1 c;
	c.foo ();

	return 0;
}

