		int x,
		int y
		)
	{
		printf("OpaqueTest.construct (%d, %d)\n", x, y);
	}

	void foo()
	{
		printf("OpaqueTest.foo ()\n");
	}

	void bar()
	{
		printf("OpaqueTest.bar ()\n");
	}
