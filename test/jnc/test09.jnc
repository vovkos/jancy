bool foo ()
{
	printf ("foo ()\n");
	return false;
}

bool bar ()
{
	printf ("bar ()\n");
	return true;
}

int main ()
{
	printf ("main ()\n");

	if (foo () && bar ())
		printf ("&& true\n");
	else
		printf ("&& false\n");

	if (foo () || bar ())
		printf ("|| true\n");
	else
		printf ("|| false\n");

	return 0;
}

