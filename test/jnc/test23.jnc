class C1
{
	int m_Int;

	reactor m_MyAutoEv
	{
		m_Int = gx_MyProp * 2;
	}
}

int bindable gx_MyProp;

int
main()
{
	printf("main ()\n");

	C1 c;
	c.m_MyAutoEv.start();

	gx_MyProp = 12;

	printf("c.m_Int = %d\n", c.m_Int);

	return 0;
}

foo()
{
	printf("foo ()\n");
}

bar()
{
	printf("bar ()\n");
}

