import "std_String.jnc"

//..............................................................................

int main ()
{
	printf ("main\n");

	std.StringBuilder stringBuilder;
	stringBuilder := "hui ";
	stringBuilder += "govno ";
	stringBuilder += "i ";
	stringBuilder += "muravei!";

	char const* string = stringBuilder.detachString ();

	printf ("string = '%s'\n", string);
	assert (streq (string, "hui govno i muravei!"));

	return 0;
}

