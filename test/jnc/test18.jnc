class C1
{
	virtual void foo()
	{
		printf("C1.foo ()\n");
	}
}

class C2: C1
{
	override void foo()
	{
		basetype1.foo();
		printf("C2.foo ()\n");
	}
}

int main()
{
	printf("main ()\n");

	C2 c;
	c.foo();

	return 0;
}

