enum State
{
	Closed,
	Opened,
	Bridged = Opened + 1
}

bitflag enum Flags
{
	ReadOnly,
	Exclusive,
	All = ReadOnly | Exclusive
}

int main ()
{
	printf ("main ()\n");

	printf ("State.Bridged = %d\n", State.Bridged);
	printf ("Flags.All = 0x%x\n", Flags.All);

	return 0;
}

